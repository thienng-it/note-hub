{
  "name": "notehub-backend",
  "version": "1.2.0",
  "description": "NoteHub Backend - Node.js/Express API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:js": "node src/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "seed": "node scripts/seed_db.js",
    "typecheck": "tsc --noEmit",
    "generate:collections": "node scripts/generate-collections.js",
    "docs": "echo 'OpenAPI docs available at /api/docs when server is running'"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.0",
    "@simplewebauthn/server": "^13.2.2",
    "@types/compression": "^1.8.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.6.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "googleapis": "^144.0.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "marked": "^15.0.4",
    "multer": "^2.0.2",
    "mysql2": "^3.11.4",
    "openapi-types": "^12.1.3",
    "otplib": "^12.0.1",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.4",
    "sanitize-html": "^2.13.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.18.3",
    "winston-loki": "^6.2.2"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@biomejs/biome": "^2.3.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.2",
    "@types/qrcode": "^1.5.6",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "openapi-to-postmanv2": "^4.18.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "js-yaml": "^4.1.1"
  }
}
